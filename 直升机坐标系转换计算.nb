Cell["\<\
@article{diebelRepresentingAttitudeEuler2006,
  title = {Representing Attitude: {{Euler}} Angles, Unit Quaternions, and \
Rotation Vectors},
  author = {Diebel, James},
  year = {2006},
  journal = {Matrix},
  volume = {58},
  pages = {1--35},
  issn = {14602431},
  abstract = {We present the three main mathematical constructs used to \
represent the attitude of a rigid body in three- dimensional space. These are \
(1) the rotation matrix, (2) a triple of Euler angles, and (3) the unit \
quaternion. To these we add a fourth, the rotation vector, which has many of \
the benefits of both Euler angles and quaternions, but neither the \
singularities of the former, nor the quadratic constraint of the latter. \
There are several other subsidiary representations, such as Cayley-Klein \
parameters and the axis-angle representation, whose relations to the three \
main representations are also described. Our exposition is catered to those \
who seek a thorough and unified reference on the whole subject; detailed \
derivations of some results are not presented.},
  isbn = {9788073753313},
  pmid = {18182420},
  keywords = {attitude,bank,cardan angles,cayley-klein parameters,di-,euler \
angles,euler-rodrigues parameters,heading,nautical \
angles,nutation,orientation,pitch,precession,quaternion,rection cosine \
matrix,roll,rotation matrix,rotation vector,slerp,spin,tait-bryan \
angles,transformation matrix,yaw},
  file = {E\\:\\\\Zotero\\\\storage\\\\966I9SRJ\\\\Diebel - Representing \
Attitude Euler Angles, Unit Quaterni.pdf}
}\
\>", "Text",
